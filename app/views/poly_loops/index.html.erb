<h1>Listing poly_loops</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @poly_loops.each do |poly_loop| %>
      <tr>
        <td><%= poly_loop.name %></td>
        <td><%= link_to 'Show', poly_loop %></td>
        <td><%= link_to 'Edit', edit_poly_loop_path(poly_loop) %></td>
        <td><%= link_to 'Destroy', poly_loop, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Poly loop', new_poly_loop_path %>
