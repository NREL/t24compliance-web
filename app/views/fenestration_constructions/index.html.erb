<h1>Listing fenestration_constructions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Fenestration type</th>
      <th>Fenestration product type</th>
      <th>Assembly context</th>
      <th>Certification method</th>
      <th>Skylight glazing</th>
      <th>Skylight curb</th>
      <th>Operable window configuration</th>
      <th>Greenhouse garden window</th>
      <th>Fenestration framing</th>
      <th>Fenestration panes</th>
      <th>Glazing tint</th>
      <th>Window divider</th>
      <th>Diffusing</th>
      <th>Shgc</th>
      <th>Shgc center of glass</th>
      <th>U factor</th>
      <th>U factor center of glass</th>
      <th>Visible transmittance</th>
      <th>Visible transmittance center of glass</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fenestration_constructions.each do |fenestration_construction| %>
      <tr>
        <td><%= fenestration_construction.name %></td>
        <td><%= fenestration_construction.fenestration_type %></td>
        <td><%= fenestration_construction.fenestration_product_type %></td>
        <td><%= fenestration_construction.assembly_context %></td>
        <td><%= fenestration_construction.certification_method %></td>
        <td><%= fenestration_construction.skylight_glazing %></td>
        <td><%= fenestration_construction.skylight_curb %></td>
        <td><%= fenestration_construction.operable_window_configuration %></td>
        <td><%= fenestration_construction.greenhouse_garden_window %></td>
        <td><%= fenestration_construction.fenestration_framing %></td>
        <td><%= fenestration_construction.fenestration_panes %></td>
        <td><%= fenestration_construction.glazing_tint %></td>
        <td><%= fenestration_construction.window_divider %></td>
        <td><%= fenestration_construction.diffusing %></td>
        <td><%= fenestration_construction.shgc %></td>
        <td><%= fenestration_construction.shgc_center_of_glass %></td>
        <td><%= fenestration_construction.u_factor %></td>
        <td><%= fenestration_construction.u_factor_center_of_glass %></td>
        <td><%= fenestration_construction.visible_transmittance %></td>
        <td><%= fenestration_construction.visible_transmittance_center_of_glass %></td>
        <td><%= link_to 'Show', fenestration_construction %></td>
        <td><%= link_to 'Edit', edit_fenestration_construction_path(fenestration_construction) %></td>
        <td><%= link_to 'Destroy', fenestration_construction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

