<h1>Listing curve_cubics</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Curve coefficient1</th>
      <th>Curve coefficient2</th>
      <th>Curve coefficient3</th>
      <th>Curve coefficient4</th>
      <th>Curve maximum out</th>
      <th>Curve maximum var1</th>
      <th>Curve minimum out</th>
      <th>Curve minimum var1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @curve_cubics.each do |curve_cubic| %>
      <tr>
        <td><%= curve_cubic.name %></td>
        <td><%= curve_cubic.curve_coefficient1 %></td>
        <td><%= curve_cubic.curve_coefficient2 %></td>
        <td><%= curve_cubic.curve_coefficient3 %></td>
        <td><%= curve_cubic.curve_coefficient4 %></td>
        <td><%= curve_cubic.curve_maximum_out %></td>
        <td><%= curve_cubic.curve_maximum_var1 %></td>
        <td><%= curve_cubic.curve_minimum_out %></td>
        <td><%= curve_cubic.curve_minimum_var1 %></td>
        <td><%= link_to 'Show', curve_cubic %></td>
        <td><%= link_to 'Edit', edit_curve_cubic_path(curve_cubic) %></td>
        <td><%= link_to 'Destroy', curve_cubic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

