<h1>Listing curve_cubics</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @curve_cubics.each do |curve_cubic| %>
      <tr>
        <td><%= curve_cubic.name %></td>
        <td><%= link_to 'Show', curve_cubic %></td>
        <td><%= link_to 'Edit', edit_curve_cubic_path(curve_cubic) %></td>
        <td><%= link_to 'Destroy', curve_cubic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Curve cubic', new_curve_cubic_path %>
