<h1>Listing coil_coolings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coil_coolings.each do |coil_cooling| %>
      <tr>
        <td><%= coil_cooling.name %></td>
        <td><%= coil_cooling.type %></td>
        <td><%= link_to 'Show', coil_cooling %></td>
        <td><%= link_to 'Edit', edit_coil_cooling_path(coil_cooling) %></td>
        <td><%= link_to 'Destroy', coil_cooling, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coil cooling', new_coil_cooling_path %>
