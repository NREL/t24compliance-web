<h1>Listing underground_walls</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Status</th>
      <th>Area</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @underground_walls.each do |underground_wall| %>
      <tr>
        <td><%= underground_wall.name %></td>
        <td><%= underground_wall.status %></td>
        <td><%= underground_wall.area %></td>
        <td><%= link_to 'Show', underground_wall %></td>
        <td><%= link_to 'Edit', edit_underground_wall_path(underground_wall) %></td>
        <td><%= link_to 'Destroy', underground_wall, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Underground wall', new_underground_wall_path %>
